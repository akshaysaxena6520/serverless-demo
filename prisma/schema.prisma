generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  fname    String?
  id       Int  @id @default(autoincrement())
  lname    String?
  email    String
  password String?
}
